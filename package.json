{
  "name": "horse-race-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bonfida/spl-name-service": "^3.0.7",
    "@firebase/analytics": "^0.10.10",
    "@firebase/app": "^0.10.16",
    "@firebase/firestore": "^4.7.5",
    "@jup-ag/api": "^6.0.30",
    "@jup-ag/core": "^4.0.0-beta.21",
    "@solana/spl-name-service": "^0.2.1",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/web3.js": "^1.95.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "buffer": "^6.0.3",
    "emoji-picker-react": "^4.12.0",
    "firebase": "^11.0.2",
    "firebase-storage": "^1.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:clean": "rm -rf node_modules/.cache && npm run dev",
    "dev:debug": "react-scripts --inspect start",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,md,json}'",
    "dev:hot": "FAST_REFRESH=true react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build",
    "dev:prod": "npm run build && npm run serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "prettier": "^3.5.2",
    "serve": "^14.2.4",
    "source-map-explorer": "^2.5.3"
  }
}
